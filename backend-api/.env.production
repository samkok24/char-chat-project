# 데이터베이스 설정 (SQLite for production)
DATABASE_URL=sqlite+aiosqlite:///./production.db

# Redis 설정 (선택사항)
REDIS_URL=redis://localhost:6379/0

# JWT 설정
JWT_SECRET_KEY=production-super-secret-key-change-this-in-real-production
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30

# AI API 키 (실제 키 필요)
GEMINI_API_KEY=
OPENAI_API_KEY=
GOOGLE_VERTEX_PROJECT_ID=
CLAUDE_API_KEY=

# 환경 설정
ENVIRONMENT=production
DEBUG=false

